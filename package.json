{
  "name": "@perspect3vism/ad4m-cli",
  "version": "0.0.4",
  "description": "AD4M command-line interface to start and remote-control your local AD4M executor",
  "main": "src/index.js",
  "bin": {
    "@perspectivism/ad4m": "bin/ad4m",
    "ad4m": "bin/ad4m"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ad4m": "node bin/ad4m",
    "test": "jest",
    "postinstall": "ln -sf node_modules/@perspect3vism/ad4m-executor/default.nix default.nix && nix-shell --run ./scripts/createHolochainSymlinks.sh && node node_modules/@perspect3vism/ad4m-executor/scripts/get-builtin-test-langs.js"
  },
  "keywords": [
    "cli",
    "perspectivism",
    "ad4m"
  ],
  "author": "Jacob Dawid",
  "license": "CAL-1.0",
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@peculiar/webcrypto": "^1.1.7",
    "@perspect3vism/ad4m": "^0.1.17",
    "@perspect3vism/ad4m-executor": "0.1.17",
    "appdata-path": "^1.0.0",
    "esm": "^3.2.25",
    "fs-extra": "^10.0.0",
    "graphql": "^15.5.1",
    "node-pty": "^0.10.1",
    "node-wget-js": "^1.0.1",
    "react": "^17.0.2",
    "readline-sync": "^1.4.10",
    "subscriptions-transport-ws": "^0.9.19",
    "unzipper": "^0.10.11",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "jest": "^27.0.6"
  }
}
